{
  "name": "Procette",
  "version": "0.0.1",
  "description": "Process manager for Windows",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.config.prod.js --progress --profile --bail",
    "pkg": "electron-packager ./ --icon ./procette_dark.ico --no-prune --build-version 0.0.1 --win32metadata.CompanyName=\"Jsonal\" --win32metadata.FileDescription=\"Robust process manager\" --win32metadata.ProductName=\"Procette\" --win32metadata.InternalName=\"Procette\" --overwrite"
  },
  "build": {
    "appId": "npmatic",
    "asar": true,
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [
    "electron",
    "webpack",
    "react",
    "process",
    "manager",
    "node"
  ],
  "author": "Jason Hicks",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.3.17",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "better-npm-run": "0.0.11",
    "css-loader": "^0.25.0",
    "electron": "^1.3.8",
    "eslint": "^3.8.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.13.3",
    "file-loader": "^0.9.0",
    "hot-reload-server": "0.0.4",
    "html-loader": "^0.4.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.13",
    "postcss-loader": "^1.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "svgo": "^0.7.1",
    "svgo-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "css-loader": "^0.25.0",
    "electron-remote": "^1.0.3",
    "electron-window-state": "^3.1.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.9.0",
    "kmp": "0.0.3",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "node-localstorage": "^1.3.0",
    "react-redux": "^4.0.1",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "reflux": "^0.4.1",
    "style-loader": "^0.13.1",
    "tinycolor2": "^1.4.1",
    "vquery": "^4.6.0",
    "win-processes": "^0.2.0",
    "win-ps": "0.0.1"
  }
}
